{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Crawlspace",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#238be6",
    "light": "#1c7ed6",
    "dark": "#238be6",
    "anchors": {
      "from": "#238be6",
      "to": "#1c7ed6"
    }
  },
  "theme": "quill",
  "layout": "solidSidenav",
  "background": {
    "style": "grid"
  },
  "sidebar": {
    "items": "card"
  },
  "search": {
    "location": "top"
  },
  "rounded": "default",
  "codeBlock": {
    "mode": "auto"
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true
  },
  "topbarLinks": [
    {
      "name": "Sign up",
      "url": "https://crawlspace.dev"
    }
  ],
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://crawlspace.dev/app"
  },
  "anchors": [
    {
      "name": "Community",
      "icon": "discord",
      "url": "https://crawlspace.dev/community"
    },
    {
      "name": "Support",
      "icon": "envelope",
      "url": "mailto:help@crawlspace.dev"
    }
  ],
  "primaryTab": {
    "name": "Documentation"
  },
  "tabs": [
    {
      "name": "CLI reference",
      "url": "command-line"
    },
    {
      "name": "Changelog",
      "url": "changelog"
    }
  ],

  "navigation": [
    {
      "group": "Welcome",
      "pages": [
        "welcome/introduction",
        "welcome/quickstart",
        "welcome/features"
      ]
    },
    {
      "group": "Build a web crawler",
      "pages": [
        "build/overview",
        {
          "group": "Development",
          "icon": "desktop",
          "pages": [
            "build/development/schema",
            "build/development/seed",
            "build/development/handler"
          ]
        },
        "build/configuration",
        "build/deployment"
      ]
    },
    {
      "group": "Platform",
      "pages": ["platform/billing", "platform/limitations"]
    },
    {
      "group": "Changelog",
      "pages": ["changelog/product-updates"]
    }
  ],
  "footerSocials": {
    "x": "https://x.com/crawlspacedev",
    "github": "https://github.com/crawlspacedev",
    "linkedin": "https://linkedin.com/company/crawlspacedev"
  }
}
